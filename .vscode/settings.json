{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "platforms": true,
        "node_modules": true,
        "plugins": true,
        "hooks": true,
        "www": true,
        "**/pages/home": true,
        "**/pages/list": true
    }
}